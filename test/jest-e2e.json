{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "roots": ["<rootDir>"],
  "modulePaths": ["<rootDir>"],
  "moduleDirectories": ["node_modules"],
  "testEnvironment": "node",
  "testRegex": "loader.e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "testTimeout": 5000
}
